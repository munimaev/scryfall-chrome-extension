{
  "manifest_version": 3,
  "name": "Scryfall Enhancer",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "extension_icon.png"
  },
  "permissions": ["scripting"],
  "host_permissions": ["https://scryfall.com/*"],
  "content_scripts": [
    {
      "matches": ["https://scryfall.com/*"],
      "css": ["enhance.css"]
    }
  ]

}